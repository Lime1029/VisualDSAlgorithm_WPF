<Window x:Class="VisualDSAlgorithm_WPF.QueueL"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualDSAlgorithm_WPF"
        mc:Ignorable="d"
        Title="Queue--Linked List Impletation" Height="450" Width="800">
    <Canvas x:Name="canvas">
        <TextBox x:Name="textInput" Margin="40, 35" Height="23" TextWrapping="Wrap" Text="" Width="120"/>
        <Button x:Name="button" Content="Enqueue" Margin="195, 35" Width="75" Click="Click_Enqueue"/>
        <Button x:Name="button1" Content="Dequeue" Margin="305, 35" Width="75" Click="Click_Dequeue"/>
        <Button x:Name="button2" Content="Clear Queue" Margin="415, 35" Width="75" Click="Click_Clear"/>
        <Label x:Name="label" Content="Head" Margin="100, 130"/>
        <Label x:Name="label1" Content="Tail" Margin="100, 380"/>
        <Label x:Name="label2" Content="" Margin="135, 130" BorderBrush="Black" BorderThickness="1" Width="25" Height="20"/>
        <Label x:Name="label3" Content="" Margin="135, 380" BorderBrush="Black" BorderThickness="1" Width="25" Height="20"/>


    </Canvas>
</Window>
