<Window x:Name="StackByLinkedList" x:Class="VisualDSAlgorithm_WPF.StackL"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualDSAlgorithm_WPF"
        mc:Ignorable="d"
        Title="Stack--Linked List Implementation" Height="450" Width="800">
    <Canvas x:Name="canvas">
        <TextBox x:Name="textInput"  HorizontalAlignment="Left" Height="23" Margin="40,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button1" Content="Push" Click="Click_Push" HorizontalAlignment="Left" Margin="190,25,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button2" Content="Pop" Click="Click_Pop" HorizontalAlignment="Left" Margin="295,25,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button3" Content="Clean Stack" Click="Click_Clear" HorizontalAlignment="Left" Margin="400,25,0,0" VerticalAlignment="Top" Width="75"/>


        <Label x:Name="label1" Content="Top" Canvas.Left="100" Canvas.Top="130"/>
        <Label x:Name="label2" Width="20" Height="20" Margin="130, 130, 1, 1" BorderBrush="Black" BorderThickness="1"/>

    </Canvas>
</Window>
